{
    "name": "wahyudedik/laravel-alert",
    "description": "A beautiful and easy-to-use alert notification library for Laravel with Blade integration",
    "keywords": [
        "laravel",
        "alert",
        "notification",
        "blade",
        "component"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/session": "^9.0|^10.0|^11.0",
        "illuminate/mail": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "illuminate/redis": "^9.0|^10.0|^11.0",
        "pusher/pusher-php-server": "^7.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "laravel/dusk": "^7.0|^8.0",
        "orchestra/testbench": "^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Wahyudedik\\LaravelAlert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wahyudedik\\LaravelAlert\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wahyudedik\\LaravelAlert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Wahyudedik\\LaravelAlert\\Facades\\Alert",
                "Toast": "Wahyudedik\\LaravelAlert\\Facades\\Toast",
                "Modal": "Wahyudedik\\LaravelAlert\\Facades\\Modal",
                "Inline": "Wahyudedik\\LaravelAlert\\Facades\\Inline"
            }
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-coverage-clover": "phpunit --coverage-clover=coverage.xml",
        "lint": "php-cs-fixer fix --dry-run --diff",
        "lint-fix": "php-cs-fixer fix",
        "stan": "phpstan analyse",
        "stan-baseline": "phpstan analyse --generate-baseline",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "quality": [
            "@lint",
            "@stan",
            "@cs"
        ],
        "quality-fix": [
            "@lint-fix",
            "@cs-fix"
        ],
        "all": [
            "@quality",
            "@test"
        ],
        "all-fix": [
            "@quality-fix",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}