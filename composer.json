{
    "name": "wahyudedik/laravel-alert",
    "description": "A beautiful and easy-to-use alert notification library for Laravel with Blade integration",
    "keywords": [
        "laravel",
        "alert",
        "notification",
        "blade",
        "component"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wahyudedik\\LaravelAlert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wahyudedik\\LaravelAlert\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wahyudedik\\LaravelAlert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Wahyudedik\\LaravelAlert\\Facades\\Alert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}